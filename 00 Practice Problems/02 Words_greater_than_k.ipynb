   "source": [
    "with open('../00 Files_Datasets/text_file.txt', 'r') as file:\n",
    "    words = file.read().split()\n",
    "\n",
    "k = int(input(\"Enter the value of k (length): \"))\n",
    "for word in words:\n",
    "    if len(word)>k:\n",
    "        print(word)"
